{
  "version": "1.0.0-*",
  "description": "Command line tool to component bind for source file changes during development.",
  "packOptions": {
    "tags": [
      "dotnet",
      "cbind"
    ]
  },
  "buildOptions": {
    "outputName": "dotnet-cbind",
    "warningsAsErrors": true,
    "emitEntryPoint": true
  },

  "dependencies": {
    "Microsoft.DotNet.Cli.Utils": "1.0.0-preview4-004233",
    "Microsoft.DotNet.ProjectModel": "1.0.0-rc4-003206",
    "Microsoft.Extensions.CommandLineUtils": "1.1.0",
    "Microsoft.Extensions.Logging": "1.1.0",
    "Microsoft.Extensions.Logging.Abstractions": "1.1.0",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.1.0"
    }
  },

  "frameworks": {
    "netcoreapp1.0": {}
  }
}